   "source": [
    "def find_squared_root(a):\n",
    "    EPSILON = 0.001\n",
    "    x = a\n",
    "    while True:\n",
    "        x_n = x - (x**2 - a)/(2*x)\n",
    "        if abs(x_n - x) < EPSILON:\n",
    "            break\n",
    "        x = x_n\n",
    "        \n",
    "    return x_n\n",
    "\n",
    "find_squared_root(3)\n",
    "        "
   ]
  }
